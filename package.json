{
  "name": "WTF-Adventure",
  "version": "1.0.6",
  "description": "A modern HTML5 multiplayer adventure!",
  "keywords": [
    "mmo",
    "mmorpg",
    "html5",
    "multiplayer",
    "nodejs",
    "browserquest",
    "javascript",
    "es6",
    "canvas"
  ],
  "homepage": "https://github.com/design1online/WTF-Adventure",
  "bugs": {
    "url": "https://github.com/design1online/WTF-Adventure/issues"
  },
  "contributors": [
    "Flavius Poenaru (https://kaetram.com)",
    "Noctisdark (https://github.com/Noctisdark)",
    "TassadarX (https://github.com/TassadarX)",
    "Greta Krafsig (https://github.com/design1online)",
    "Eric Hodges (https://github.com/eric256)",
    "Spoobert (https://github.com/spoobert)"
  ],
  "license": "wtfpl-2.0",
  "scripts": {
    "start": "webpack --mode=development",
    "wtfserver": "nodemon --exec babel-node src/server/js/main.js",
    "deploy": "webpack --mode=production --config webpack.production.config.js",
    "lint": "eslint ./src/**/**.js",
    "mysql-start": "mysql-ctl start",
    "wtf": "npm run mysql-start && npm start",
    "esdocs": "./node_modules/.bin/esdoc",
    "docs": "open ./docs/index.html",
    "test": "jest --env=jsdom --coverage && npm run badges",
    "badges": "make-coverage-badge"
  },
  "engines": {
    "node": ">=8.0"
  },
  "engineStrict": true,
  "nodemonConfig": {
    "ignore": [
      "src/client/*",
      "assets/*",
      "build/*",
      "css/*"
    ],
    "delay": "2500"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.5.3",
    "commander": "^2.20.3",
    "connect": "^3.7.0",
    "http": "0.0.0",
    "jquery": "^3.5.1",
    "log": "^1.4.0",
    "memcache": "^0.3.0",
    "modernizr": "^3.11.3",
    "modernizr-webpack-plugin": "^1.0.7",
    "mysql": "^2.18.1",
    "popper.js": "^1.16.1",
    "request": "^2.88.2",
    "require": "^2.4.20",
    "sanitizer": "^0.1.3",
    "semver": "^5.7.1",
    "serve-static": "^1.14.1",
    "shutdown-hook": "^1.1.7",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "underscore": "^1.11.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.1",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.1",
    "file-loader": "^3.0.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jest": "^25.5.4",
    "make-coverage-badge": "^1.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.1.1",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.10"
  }
}
